    "def utilfixpoint(V, start, end):\n",
    "    if start > end:\n",
    "        return False\n",
    "    m = (start + end) // 2\n",
    "    if V[m] == m:\n",
    "        return True\n",
    "    elif V[m] > m:\n",
    "        return utilfixpoint(V, start, m - 1)\n",
    "    else:\n",
    "        return utilfixpoint(V, m + 1, end)\n",
    "    \n",
    "def fixpoint(V):\n",
    "    return utilfixpoint(V, 0, len(V) - 1)"
